{
  "name": "create-fullstack-app",
  "version": "0.0.0-development",
  "description": "Create Fullstack TypeScript application with ease.",
  "author": "Divlo <contact@divlo.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Divlo/create-fullstack-app.git"
  },
  "bugs": {
    "url": "https://github.com/Divlo/create-fullstack-app/issues"
  },
  "homepage": "https://github.com/Divlo/create-fullstack-app#readme",
  "keywords": [
    "cli",
    "fullstack",
    "template",
    "react",
    "next",
    "vue",
    "nuxt",
    "express",
    "sequelize",
    "strapi",
    "nest"
  ],
  "main": "build/index.js",
  "bin": "build/index.js",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "build",
    "templates"
  ],
  "ts-standard": {
    "ignore": [
      "build",
      "node_modules",
      "templates",
      "app-test"
    ],
    "envs": [
      "node",
      "jest"
    ],
    "report": "stylish"
  },
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "node build/index.js app-test",
    "dev": "tsc --watch",
    "lint:commit": "commitlint",
    "lint:editorconfig": "editorconfig-checker",
    "lint:markdown": "markdownlint '**/*.md' --dot --ignore node_modules --ignore app-test",
    "lint:typescript": "ts-standard",
    "release": "semantic-release",
    "test": "jest",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "chalk": "4.1.1",
    "child-process-promise": "2.2.1",
    "clipanion": "3.0.1",
    "inquirer": "8.1.2",
    "log-symbols": "4.1.0",
    "make-dir": "3.1.0",
    "ora": "5.4.1",
    "rimraf": "3.0.2",
    "tslib": "2.3.0",
    "update-notifier": "5.1.0",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@types/child-process-promise": "2.2.2",
    "@types/inquirer": "7.3.3",
    "@types/jest": "26.0.24",
    "@types/mock-fs": "4.13.1",
    "@types/node": "16.4.10",
    "@types/rimraf": "3.0.1",
    "@types/update-notifier": "5.1.0",
    "@types/validate-npm-package-name": "3.0.3",
    "editorconfig-checker": "4.0.2",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "markdownlint-cli": "0.28.1",
    "mock-fs": "5.0.0",
    "semantic-release": "17.4.4",
    "pinst": "2.1.6",
    "ts-jest": "27.0.4",
    "ts-standard": "10.0.0",
    "typescript": "4.3.5"
  }
}
